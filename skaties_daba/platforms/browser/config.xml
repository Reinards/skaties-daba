<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Skaties Dabā</name>
    <description>
        Skaties dabā - spēle, kas izveidota sadarbībā ar Cemex Iespēju Fondu.
    </description>
    <author email="reinardssaulitis@gmail.com" href="https://reinards.github.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
